{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
        "cronJob": {
            "properties": {
                "affinity": {
                    "properties": {},
                    "type": "object"
                },
                "resources": {
                    "properties": {
                        "limits": {
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "requests": {
                            "properties": {
                                "cpu": {
                                    "type": "string"
                                },
                                "memory": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "type": "object"
                },
                "schedule": {
                    "type": "string"
                },
                "securityContext": {
                    "properties": {
                        "allowPrivilegeEscalation": {
                            "type": "boolean"
                        },
                        "capabilities": {
                            "properties": {
                                "drop": {
                                    "items": {
                                        "type": "string"
                                    },
                                    "type": "array"
                                }
                            },
                            "type": "object"
                        },
                        "readOnlyRootFilesystem": {
                            "type": "boolean"
                        },
                        "runAsNonRoot": {
                            "type": "boolean"
                        },
                        "runAsUser": {
                            "type": "integer"
                        },
                        "seccompProfile": {
                            "properties": {
                                "type": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        }
                    },
                    "type": "object"
                },
                "successfulJobsHistoryLimit": {
                    "type": "integer"
                },
                "tolerations": {
                    "properties": {},
                    "type": "object"
                }
            },
            "type": "object"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "properties": {
                "pullPolicy": {
                    "enum": [
                        "IfNotPresent",
                        "Always",
                        "Never"
                    ],
                    "type": "string"
                },
                "repository": {
                    "pattern": "^[a-z0-9./-]+$",
                    "type": "string"
                }
            },
            "type": "object"
        },
        "nameOverride": {
            "type": "string"
        },
        "serviceAccount": {
            "properties": {
                "annotations": {
                    "items": {
                        "type": "object"
                    },
                    "properties": {},
                    "type": "object"
                },
                "automount": {
                    "type": "boolean"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}
